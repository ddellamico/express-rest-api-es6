{
  "name": "express-rest-api-es6",
  "version": "0.0.1",
  "description": "Express & ES6 REST API Boilerplate",
  "author": {
    "name": "Damien Dell'Amico",
    "email": "damien.dellamico@gmail.com",
    "url": "https://github.com/ddellamico"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node src/debug.js",
    "babel": "babel 'src' --out-dir dist --source-maps",
    "dev": "nodemon ./src/debug.js -L --watch 'src/**/*' --ext 'js'",
    "debug": "NODE_PORT=3001 iron-node src/debug.js",
    "prod": "node ./dist/index",
    "lint": "jshint src/**/*.js",
    "outdated": "npm outdated --depth 0",
    "prepush": "npm shrinkwrap && npm test",
    "postmerge": "npm install"
  },
  "pre-commit": [
    "lint",
    "outdated"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ddellamico/express-rest-api-es6"
  },
  "engines": {
    "node": ">=5.x",
    "npm": ">=2.x"
  },
  "bugs": {
    "url": "https://github.com/ddellamico/express-rest-api-es6/issues"
  },
  "files": [
    "src",
    "README.md"
  ],
  "keywords": [
    "babel",
    "es6",
    "node",
    "express"
  ],
  "dependencies": {
    "bluebird": "^3.1.1",
    "body-parser": "^1.12.2",
    "co-express": "^1.2.1",
    "cors": "^2.7.1",
    "crypto": "~0.0.3",
    "dotenv": "^1.2.0",
    "express": "^4.13.3",
    "helmet": "^0.7.0",
    "jsonwebtoken": "^5.5.0",
    "lodash": "^3.5.0",
    "method-override": "^2.3.2",
    "mongoose": "^4.2.9",
    "morgan": "^1.5.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "redis": "^2.4.2",
    "validator": "^3.41.2",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "babel-core": "~6.3.13",
    "babel-preset-es2015-node5": "^1.1.1",
    "jshint": "latest",
    "nodemon": "^1.8.1",
    "nsp": "^2.0.0",
    "pre-commit": "^1.1.2",
    "source-map-support": "^0.4.0"
  },
  "license": {
    "type": "MIT"
  }
}
